    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug mono-KITTI",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Examples/Monocular/mono_kitti",
            "args": [
                "/home/kay/dev/ORB_SLAM3/Vocabulary/ORBvoc.txt",
                "/home/kay/dev/ORB_SLAM3/Examples/Monocular/KITTI00-02.yaml",
                "~/Datasets/KITTI/odometry_gray/dataset/sequences/00"
            ],
            "cwd": "${fileDirname}",
            "externalConsole": false,
            "miDebuggerPath": "/usr/bin/gdb",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}